{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "typedef-whitespace": [
        true
      ],
      "semicolon": [true, "never"],
      "space-before-function-paren": [true, "always"],
      "quotemark": [true, "single"],
      "trailing-comma": [false],
      "prefer-const": false,
      "no-console": [false]
    },
    "rulesDirectory": []
}